{
  "name": "deku-slide",
  "version": "1.2.0",
  "description": "Carousel component for Deku",
  "license": "MIT",
  "repository": "gillstrom/deku-slide",
  "author": {
    "name": "Andreas GillstrÃ¶m",
    "email": "andreasgillstrom@gmail.com",
    "url": "github.com/gillstrom"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "browserify -o example/build.js -d example/index.js -t [ babelify --presets [ es2015 react ] ]",
    "prepublish": "babel --presets es2015,react --out-file dist.js index.js ",
    "test": "xo"
  },
  "main": "dist.js",
  "styles": "index.css",
  "files": [
    "dist.js",
    "index.css"
  ],
  "keywords": [
    "carousel",
    "component",
    "deku",
    "deku-component",
    "slide",
    "slider",
    "slideshow",
    "swipe",
    "swiper",
    "touch"
  ],
  "dependencies": {
    "condense-keys": "^1.1.0",
    "magic-virtual-element": "^1.0.6",
    "object-assign": "^4.0.1",
    "swiper": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "deku": "^1.0.0",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.15.0",
    "xo": "*"
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ],
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "dist.js"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/no-unknown-property": 0,
      "react/prop-types": 0,
      "quote-props": 0
    }
  }
}
